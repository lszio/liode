{
  "name": "wedex",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "run-p -l *:watch",
    "build": "run-s *:build",
    "compile": "run-s *:compile",
    "repl": "shadow-cljs browser-repl",
    "cljs:watch": "shadow-cljs watch app",
    "cljs:build": "shadow-cljs compile app",
    "cljs:compile": "shadow-cljs release app",
    "css:build": "cross-env postcss src/css/tailwind.css -o ./public/css/style.css --verbose",
    "css:watch": "cross-env postcss src/css/tailwind.css -o ./public/css/style.css --verbose -w",
    "css:compile": "cross-env postcss NODE_ENV=production src/css/tailwind.css -o ./public/css/style.css --verbose -w"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "postcss-cli": "^10.1.0",
    "shadow-cljs": "2.25.2",
    "tailwindcss": "^3.3.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.33.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
